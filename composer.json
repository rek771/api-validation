{
  "name": "id.shabunin/api-validation",
  "description": "Check text by patterns API",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^6.0.7",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Coolcigarets\\ApiValidation\\": "app",
      "Coolcigarets\\Framework\\": "framework"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
